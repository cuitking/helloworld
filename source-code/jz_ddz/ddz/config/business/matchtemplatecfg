	[305]	= {  --12:30 聚众金卡赛
		id=305,
	    name = "聚众金卡赛",
	    des = "1.每三分钟升盲一次\n2.赛前30分钟允许入场，请提前进场准备\n3.开赛后15分钟内允许报名入场\n4.比赛中途退出视为退赛 \n5.比赛不允许重购\n6.比赛不允许增购\n7.比赛开始后剩余3人则不能进入赛场",
	    picture_url = "",
	    match_type = 6,
	    style = {
	    	bg_name = "5.png",
	    	title_name = "jys.png",--可以直接配置汉字，如果不使用现有图片的话
	    	sub_title_bg = "9.png",
	    	sub_title = "金卡赛",--左上角显示
	    	suffix_bg = "",
            suffix_title = "",--右下角现实
	    },

	    start_time = "2016-6-7 11:00:00",

	    --显示排序优先级
	    match_seq = 2,

	    --报名条件
	    signing_conditions = {
	    	[1] = {
	      		id=1,
	      		num=500,
	    	},
	    },
	    --排名奖励模板
	    award_template_id = 115,
	    --循环模板
	    cycle_template_id = 2,
	    --涨盲模板
	    blind_template_id = 4,
	    --初始筹码
	    match_init_chips = 6000,
	    --比赛最大旁观人数
	    max_wait_num = 500,
	    action_timeout = 2000,
	    --比赛开始时单桌最大玩家数
	    start_max_table_playernum = 6,
	    --比赛的最大报名人数
	    match_max_player_num = 2000,
	    --比赛的最少开赛人数
	    match_min_player_num = 3,
	    --用于不同比赛服务器加载不同比赛
	    match_svrd_id = "",

	    --用于已报名玩家开赛前提醒单位s
	    match_remind_time = {
	    	[1] = 60,
	    },
		--比赛开始前相对开始时间的报名开始时间单位s
		match_signup_start_time = 86400,
	    --比赛开始前相对开始时间的报名结束时间单位s
	    match_signup_end_time = 10,
	    conf_version = 14,
	    
	    --比赛未报名玩家开赛前提醒
	    match_remind_all_time = {
	    	[1] = 600,
	    	[2] = 300,
	    	[3] = 240,
	    },

	    --比赛结束后多长时间后从列表中删除 单位s
	    matchend_del_time=86400,
	    --比赛开赛前多长时间可以进入比赛单位s
	    match_enter_time_before=600,

		--比赛开始前什么时候推送
		--如果在这个时间之后报名，则在开赛那一刻推送
		match_push_notice_time=300,
		
	    --比赛上线时间
	    match_show_time="2016-3-20 12:00:00",
	    --比赛下线时间
	    match_off_time="2016-12-30 2:00:00",
	    --改选项用于指定是否打印日志
	    is_debug = false,
	   	--是否仅允许过百玩家参加
		is_onlywhite = false,
		--比赛截止进入人数
		stop_enter_playernum = 5,
		--支持rebuy盲注阶段配置
		rebuy = {
			begin_blind_index = 1,
			end_blind_index = 5, --rebuy盲注级别控制延时报名和延时进入时间，算法 时间=累加盲注表的1到end_blind_index盲注级别的time
			num = 2,
		},

		--rebuy条件
	    rebuy_conditions = {
	    	[1] = {  --索引是次数
	      		id=1,
	      		num=500,
	    	},
	    	[2] = {		--索引是次数
	      		id=1,
	      		num=500,
	    	},
	    	[3] = {		--索引是次数
	      		id=1,
	      		num=500,
	    	},
	    },

		--支持addbuy盲注阶段配置
		addbuy = {
			begin_blind_index = 6,
			end_blind_index = 6,
			num = 0,			
			addbuy_chips = 5000,
		},

		addbuy_conditions = {
	    	[1] = {		--索引是次数
	      		id=1,
	      		num=1000,
	    	},
	    	[2] = {		--索引是次数
	      		id=1,
	      		num=1000,
	    	},
	    	[3] = {		--索引是次数
	      		id=1,
	      		num=1000,
	      	}
	    },
		
		--筹码为0时踢出倒计时(单位s)
		kicked_time = 40, 					    
		--连续超时判定次数
		continuous_timeout = 2,
		match_type_flag = {
	    	"日常赛",
	    },
