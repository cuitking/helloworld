		--比赛截止进入人数
		stop_enter_playernum = 5,
		--支持rebuy盲注阶段配置
		rebuy = {
			begin_blind_index = 1,
			end_blind_index = 6,--rebuy盲注级别控制延时报名和延时进入时间，算法 时间=累加盲注表的1到end_blind_index盲注级别的time
			num = 4,
		},

		--rebuy条件
	    rebuy_conditions = {
	    	[1] = {  --索引是次数
	      		id=1,
	      		num=100,
	    	},
	    	[2] = {		--索引是次数
	      		id=1,
	      		num=100,
	    	},
	    	[3] = {		--索引是次数
	      		id=1,
	      		num=100,
	    	},
	    	[4] = {		--索引是次数
	      		id=1,
	      		num=100,
	    	},
	    },

		--支持addbuy盲注阶段配置
		addbuy = {
			begin_blind_index = 7,
			end_blind_index = 7,
			num = 1,			
			addbuy_chips = 10000,
		},

		addbuy_conditions = {
	    	[1] = {		--索引是次数
	      		id=1,
	      		num=100,
	    	},
	    	[2] = {		--索引是次数
	      		id=1,
	      		num=100,
	    	},
	    	[3] = {		--索引是次数
	      		id=1,
	      		num=100,
	      	}
	    },
		
		--筹码为0时踢出倒计时(单位s)
		kicked_time = 40, 					    
		--连续超时判定次数
		continuous_timeout = 2,
		match_type_flag = {
	    	"日常赛","大奖赛",
	    },
	    isopen_floadpool = false,
